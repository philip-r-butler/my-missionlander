{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "util": "./src/util",
          "pages": "./src/pages",
          "assets": "./src/assets",
          "images": "./src/assets/images",
          "fonts": "./src/assets/fonts/coop-bank-regular",
          "scss": "./src/assets/scss"
        }
      }
    ]
  ]
}
