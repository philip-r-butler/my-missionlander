{
  "name": "my-missionlander",
  "version": "1.0.0",
  "description": "Mission Lander Game",
  "main": "index.js",
  "repository": "git@github.com:philip-r-butler/my-missionlander.git",
  "author": "Philip R Butler <philip-r-butler@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --open --hot --config webpack.dev.js",
    "start:uat": "webpack-dev-server --open --hot --config webpack.uat.js",
    "start:prod": "webpack-dev-server --open --hot --config webpack.prod.js",
    "lint": "eslint src --max-warnings 5",
    "prettier": "yarn prettier:code && yarn prettier:styles",
    "prettier:code": "prettier-eslint --write \"src/**/*.{js,jsx}\" ",
    "prettier:styles": "prettier-stylelint --write \"src/**/*.{css,scss}\" ",
    "test": "yarn lint && yarn test:coverage",
    "test:only": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest src --watch",
    "test:update": "jest --updateSnapshot",
    "build": "yarn build:prod",
    "build:dev": "webpack --config webpack.dev.js",
    "build:test": "yarn lint && yarn test:coverage && webpack --config webpack.uat.js",
    "build:prod": "yarn lint && yarn test:coverage && webpack --config webpack.prod.js",
    "build:watch": "webpack --watch --display-error-details --config webpack.dev.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "jest-watch-typeahead": "^0.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-format": "^23.6.0",
    "regenerator-runtime": "^0.12.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "copy-webpack-plugin": "^5.1.1",
    "date-fns": "^1.30.1",
    "lodash": "^4.17.11"
  }
}
